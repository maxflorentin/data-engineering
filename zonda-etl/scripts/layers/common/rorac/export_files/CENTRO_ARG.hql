SELECT DISTINCT RE.cod_centro_cont, UPPER(CEN.des_hijo) ds_ren_centro FROM (SELECT RE.fec_data , RE.cod_centro_cont FROM bi_corp_staging.rio157_ms0_ft_dwh_blce_result RE WHERE RE.partition_date = '$month_to' )RE JOIN bi_corp_staging.rio157_ms0_dm_je_dwh_centros_ctr CEN ON CEN.partition_date = '$max_partition_rio157_ms0_dm_je_dwh_centros_ctr'	AND RE.cod_centro_cont = CEN.cod_hijo ; 
{
  "table": "bi_corp_common.stk_adm_empresas_ventas_balance_cliente",
  "partition_date": "${partition_date}",
  "controls": [
    {
      "control_id": "duplicates",
      "field_to_check": ["id_adm_balance","cod_adm_penumper","id_adm_venta","dt_adm_fec_venta"]
    },
    {
      "control_id": "null_values",
      "field_to_check": ["id_adm_balance","cod_adm_penumper","id_adm_venta","dt_adm_fec_venta","fc_adm_mon_venta"],
      "threshold": 0.00
    },
    {
      "control_id": "date_format",
      "field_to_check": ["dt_adm_fec_venta"],
      "pattern": "yyyy-MM-dd HH:mm:ss.SSS",
      "threshold": 0.00
    }
  ]
}


        },
        {
            "name": "offer_state_id",
            "type": "string"
        },
        {
            "name": "tna",
            "type": "string"
        },
        {
            "name": "term",
            "type": "string"
        },
        {
            "name": "total_due_amount",
            "type": "string"
        },
        {
            "name": "creation_date",
            "type": "string"
        }
    ],
    "extraColumns": [
        {
            "name": "partition_date",
            "value": "${partition_date}",
            "type": "string",
            "partitionColumn": true,
            "partitionOrder": 1
        }
    ]
